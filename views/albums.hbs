<div class="container">
  <div class="row">
    {{#each albumInfo}}
      <div class="col-4 p-3">
        <a href="/tracks/{{id}}" class="button">
          <img src="{{images.0.url}}" alt="{{name}}" width="100%" />
        </a>

        <h3 class="title"><a href="/tracks/{{id}}" class="button">
            {{name}}</a></h3>
        <a href="/tracks/{{id}}" class="button">View Tracks</a>
      </div>
    {{/each}}
  </div>
</div>
<style>
  .title a { text-decoration: none; color: black; }
</style>